{
  "manifest_version": 2,

  "name": "The One Ring for Roll20",
  "description": "A simple fantasy/The One Ring RPG-based theme for Roll20.net.",
  "version": "1.0",

  "icons": {
    "16": "icons/theonering-16.png",
    "48": "icons/theonering-48.png",
    "128": "icons/theonering-128.png"
  },

  "content_scripts": [
    {
      "matches": ["https://app.roll20.net/editor/"],
      "css": ["/mystyles.css"],
      "js": ["/theoperator.js"]
    }
  ],

  "permissions": [
    // "https://secure.flickr.com/"
  ],
  "browser_action": {
    // "default_icon": "icon.png",
    // "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "/manifest.json",
    "/theoperator.js",
    "/mystyles.css"
  ]
}